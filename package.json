{
  "name": "discord-bot-template",
  "version": "1.0.0",
  "description": "A template for a basic Discord Bot using TypeScript",
  "main": "index.tsc",
  "scripts": {
    "start": "node dist/index.js",
    "build": "npm run format && npm run lint && npx tsc",
    "lint": "eslint ./src",
    "format": "prettier . --write",
    "doc": "npx typedoc --entryPointStrategy expand ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EAnathos/CustomQuizz.git"
  },
  "author": "EAnathos",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EAnathos/CustomQuizz/issues"
  },
  "homepage": "https://github.com/EAnathos/CustomQuizz#readme",
  "dependencies": {
    "@types/node-schedule": "^2.1.7",
    "discord.js": "^14.15.2",
    "dotenv": "^16.4.5",
    "node-schedule": "^2.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@typescript-eslint/eslint-plugin": "^8.9.0",
    "@typescript-eslint/parser": "^8.9.0",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.11.0",
    "prettier": "^3.3.3",
    "typedoc": "^0.25.13",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.9.0"
  }
}
